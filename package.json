{
  "name": "betting-system",
  "version": "1.0.0",
  "description": "Complete betting system with multiple applications",
  "private": true,
  "workspaces": [
    "backend",
    "client_panels",
    "operating-panel/apps/frontend",
    "user-management/apps/frontend"
  ],
  "scripts": {
    "dev:frontend": "concurrently \"npm run dev:client\" \"npm run dev:operating\" \"npm run dev:user-management\"",
    "dev:client": "cd client_panels && npx next dev -p 3000",
    "dev:operating": "cd operating-panel/apps/frontend && npx next dev -p 3001",
    "dev:user-management": "cd user-management/apps/frontend && npx next dev -p 3002",
    "dev:all": "npm run dev:frontend",
    "build:backend": "cd backend && echo 'Backend is Node.js, no build needed'",
    "build:client": "cd client_panels && npx next build",
    "build:operating": "cd operating-panel/apps/frontend && npx next build",
    "build:user-management": "cd user-management/apps/frontend && npx next build",
    "build:all": "npm run build:backend && npm run build:client && npm run build:operating && npm run build:user-management",
    "start:backend": "cd backend/externalapi && node index.js",
    "start:client": "cd client_panels && npx next start -p 3000",
    "start:operating": "cd operating-panel/apps/frontend && npx next start -p 3001",
    "start:user-management": "cd user-management/apps/frontend && npx next start -p 3002",
    "install:all": "npm install",
    "clean": "rimraf node_modules package-lock.json",
    "prisma:generate": "npx prisma generate --schema=./user-management/apps/frontend/prisma/schema.prisma",
    "prisma:db:push": "npx prisma db push --schema=./user-management/apps/frontend/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./user-management/apps/frontend/prisma/schema.prisma",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "lint": "npm run lint:client && npm run lint:operating && npm run lint:user-management",
    "lint:client": "cd client_panels && npx next lint",
    "lint:operating": "cd operating-panel/apps/frontend && npx next lint",
    "lint:user-management": "cd user-management/apps/frontend && npx next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "hls.js": "^1.6.10",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "next": "^14.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^11.0.5",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "stripe": "^14.7.0",
    "tailwindcss": "^3.3.0",
    "twilio": "^4.19.0",
    "typescript": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.22.0",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "betting",
    "sports",
    "casino",
    "monorepo",
    "nextjs",
    "express",
    "prisma",
    "postgresql",
    "redis"
  ],
  "author": "Your Name",
  "license": "MIT"
}
